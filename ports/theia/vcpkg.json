{
  "name": "theia",
  "version-date": "2019-07-17",
  "description": "An open source library for multiview geometry and structure from motion",
  "homepage": "https://github.com/sweeneychris/TheiaSfM",
  "license": "BSD-3-Clause",
  "supports": "!(windows & (x86 | arm64)) & !(osx & arm64)",
  "dependencies": [
    "cereal",
    {
      "name": "ceres",
      "features": [
        "suitesparse"
      ]
    },
    "flann",
    {
      "name": "freeglut",
      "platform": "!osx"
    },
    "glew",
    "openimageio",
    "rapidjson",
    "rocksdb",
    "spectra",
    "suitesparse",
    {
      "name": "vcpkg-cmake",
      "host": true
    },
    {
      "name": "vcpkg-cmake-config",
      "host": true
    }
  ]
}
